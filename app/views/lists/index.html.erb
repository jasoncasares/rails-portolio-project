<% @lists.each do |list| %>
  <%= list.name %><br>
  <%= list.due_date %><br>
  <%= link_to "Show", list_path(list) %><br>
  <%= link_to "Edit", edit_list_path(list) %><br>
  <%= link_to "Delete", list_path(list), method: :delete, data: { confirm: 'Are you sure?' } %><br>
<% end %><br>

<%= link_to 'New List', new_list_path(@list) %>
