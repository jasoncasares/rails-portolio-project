<% @lists.each do |list| %>
  <h4><%= list.name %></h4>
  <%= list.categories.name %><br>
  Due: <%= list.due_date.strftime("%B %d, %Y") %><br>
  <%= list.overdue? %><br>
  <%= link_to "Show", list_path(list) %> |
  <%= link_to "Edit", edit_list_path(list) %> |
  <%= link_to "Delete", list_path(list), method: :delete, data: { confirm: 'Are you sure?' } %><br>
<% end %><br>

<%= link_to 'New List', new_list_path(@list) %>
